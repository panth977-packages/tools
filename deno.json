{
  "name": "@panth977/tools",
  "version": "0.1.11",
  "exports": "./mod.ts",
  "license": "MIT",
  "tasks": {
    "format": "deno fmt **/*.ts",
    "lint": "deno lint --fix",
    "version:patch": "deno run -A scripts/version.ts patch",
